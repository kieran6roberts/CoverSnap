{
  "name": "cover-snap",
  "type": "module",
  "packageManager": "pnpm@9.0.6",
  "version": "1.0.0",
  "description": "Cover Snap lets you easily generate good looking cover images for your blog posts.",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "start:ci": "start-server-and-test 'remix-serve ./build/server/index.js' http://localhost:3000 'cypress run'",
    "lint": "eslint .",
    "prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "typecheck": "tsc",
    "format": "pnpm lint && pnpm prettier:fix",
    "format:all": "pnpm typecheck && pnpm lint && pnpm prettier:fix",
    "tests:open": "start-server-and-test 'pnpm dev' http://localhost:5173 'cypress open'",
    "tests:run": "start-server-and-test 'pnpm dev' http://localhost:5173 'cypress run'",
    "tests:run:ci": "cypress run"
  },
  "keywords": [],
  "author": "Kieran Roberts",
  "license": "ISC",
  "dependencies": {
    "@mantine/core": "^7.15.2",
    "@mantine/hooks": "^7.15.2",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "dom-to-image-more": "^3.5.0",
    "file-saver": "^2.0.5",
    "globals": "^15.14.0",
    "iconoir-react": "^7.10.1",
    "isbot": "4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "start-server-and-test": "^2.0.9"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.2",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "cy-verify-downloads": "^0.2.6",
    "cypress": "^13.17.0",
    "cypress-slow-down": "^1.3.1",
    "eslint": "^9.17.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
