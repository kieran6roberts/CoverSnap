{
  "name": "@cvrsnap/frontend",
  "type": "module",
  "version": "1.0.0",
  "description": "Cover Snap is a free tool that lets you easily generate good looking cover images for your blog posts in seconds using the editing dashboard.",
  "sideEffects": false,
  "author": "Kieran Roberts",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "9.0.6"
  },
  "packageManager": "pnpm@9.0.6",
  "license": "MIT",
  "scripts": {
    "build": "react-router build",
    "preview": "pnpm build && pnpm start",
    "dev": "react-router dev",
    "lint": "eslint .",
    "prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "typecheck": "react-router typegen && tsc",
    "format": "pnpm lint && pnpm prettier:fix",
    "format:all": "pnpm typecheck && pnpm lint && pnpm prettier:fix",
    "tests:open": "start-server-and-test 'pnpm dev' http://localhost:5173 'cypress open'",
    "tests:run": "start-server-and-test 'pnpm dev' http://localhost:5173 'cypress run'",
    "tests:run:ci": "cypress run"
  },
  "dependencies": {
    "@mantine/core": "^7.15.2",
    "@mantine/hooks": "^7.15.2",
    "@react-router/node": "^7.1.3",
    "@react-router/serve": "^7.0.0",
    "classnames": "^2.5.1",
    "file-saver": "^2.0.5",
    "globals": "^15.14.0",
    "hero-patterns": "^2.1.0",
    "html-to-image": "^1.11.11",
    "iconoir-react": "^7.10.1",
    "idb-keyval": "^6.2.1",
    "isbot": "4",
    "react": "^18.3.1",
    "react-confetti": "^6.2.2",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.0",
    "sonner": "^1.7.1",
    "start-server-and-test": "^2.0.9",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.3",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "cy-verify-downloads": "^0.2.6",
    "cypress": "^13.17.0",
    "cypress-slow-down": "^1.3.1",
    "eslint": "^9.17.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
